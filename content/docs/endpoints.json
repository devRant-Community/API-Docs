	{
		"method": "GET",
		"endpoint": "/get-user-id",
		"description": "Get the ID of a user.\nThe UserID is used by other endpoints, so it's widely needed!",
		"parameters": [
			{
				"name": "app",
				"description": "Standard app parameter. Just set it to 3."
			},
			{
				"name": "username",
				"description": "Username of the user."
			}
		],
		"exampleCode": [
			{
				"name": "curl",
				"langCode": "shell",
				"code": [
					"$ curl -X GET 'https://devrant.com/api/get-user-id?app=3&username=User1'"
				]
			},
			{
				"name": "PHP",
				"langCode": "php",
				"code": [
					"<?php",
					"",
					"// Parameters",
					"$username = \"User1\";",
					"",
					"// Do request",
					"$response = file_get_contents(\"https://devrant.com/api/get-user-id?app=3&username=\" . $username);",
					"",
					"echo $response;",
					"",
					"?>"
				]
			}
		],
		"exampleResponse": [
			"{",
			"  \"success\": true,",
			"  \"user_id\": 12301",
			"}"
		]
	},


	{
		"method": "GET",
		"endpoint": "/users/<userID>",
		"description": "Get the profile of a user.\nReplace <userID> with the user's ID.",
		"parameters": [
			{
				"name": "app",
				"description": "Standard app parameter. Just set it to 3."
			},
			{
				"name": "skip",
				"description": "How many rants to skip."
			},
			{
				"name": "content",
				"description": "What content to load.\nEither 'all', 'rants', 'upvoted', 'comments', 'viewed', 'favorites' or 'collabs'."
			}
		],
		"exampleCode": [
			{
				"name": "curl",
				"langCode": "shell",
				"code": [
					"$ curl -X GET 'https://devrant.com/api/users/<userID>?app=3&content=rants&skip=0'"
				]
			},
			{
				"name": "PHP",
				"langCode": "php",
				"code": [
					"<?php",
					"",
					"// Parameters",
					"$userID = 12301;",
					"$skip = 0;",
					"$content = \"rants\";",
					"",
					"// Do request",
					"$response = file_get_contents(\"https://devrant.com/api/users/\" . $userID . \"?app=3&content=\" . $content . \"&skip=\" . $skip);",
					"",
					"echo $response;",
					"",
					"?>"
				]
			}
		],
		"exampleResponse": [
			"{",
			"  \"success\": true,",
