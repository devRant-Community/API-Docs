	{
		"method": "GET",
		"endpoint": "/devrant/rants",
		"description": "Get a list of rants. (Rant feed)",
		"parameters": [
			{
				"name": "app",
				"description": "Standard app parameter. Just set it to 3."
			},
			{
				"name": "sort",
				"description": "Sort rants by 'algo', 'top' or 'recent'."
			},
			{
				"name": "range",
				"description": "In what time spectrum the rants should be. Either 'day', 'week', 'month' or 'all'."
			},
			{
				"name": "limit",
				"description": "Limit the amount of rants to load."
			},
			{
				"name": "skip",
				"description": "How many rants to skip."
			},
			{
				"name": "token_id (optional)",
				"description": "Token ID of the user"
			},
			{
				"name": "token_key (optional)",
				"description": "Token key belonging to the user"
			},
			{
				"name": "user_id (optional)",
				"description": "ID of the user on whose behalf the action is done"
			}
		],
		"exampleCode": [
			{
				"name": "curl",
				"langCode": "shell",
				"code": [
					"$ curl -X GET 'https://devrant.com/api/devrant/rants?app=3&sort=top&range=day&limit=20&skip=0'"
				]
			},
			{
				"name": "PHP",
				"langCode": "php",
				"code": [
					"<?php",
					"",
					"// Parameters",
					"$sort = 'top';",
					"$range = 'day';",
					"$limit = 20;",
					"$skip = 0;",
					"",
					"// Do request",
					"$response = file_get_contents(\"https://devrant.com/api/devrant/rants?app=3&sort=\" . $sort . \"&range=\" . $range . \"&limit=\" . $limit . \"&skip=\" . $skip);",
					"",
					"echo $response;",
					"",
					"?>"
				]
			}
		],
		"exampleResponse": [
			"{",
