				"name": "app",
				"description": "Standard app parameter. Just set it to 3."
			},
			{
				"name": "username",
				"description": "Username to login with"
			},
			{
				"name": "password",
				"description": "Password to login with"
			}
		],
		"exampleCode": [
			{
				"name": "curl",
				"langCode": "shell",
				"code": [
					"$ curl --location --request POST 'https://devrant.com/api/users/auth-token' \\",
					"--header 'Content-Type: application/x-www-form-urlencoded' \\",
					"--data-urlencode 'app=3' \\",
					"--data-urlencode 'username=username' \\",
					"--data-urlencode 'password=password'"
				]
			},
			{
				"name": "PHP",
				"langCode": "php",
				"code": [
					"<?php",
					"$url = 'https://devrant.com/api/users/auth-token';",
					"$data = array('app' => '3', 'username' => 'username', 'password' => 'password');",
					"",
					"$options = array(",
					"    'http' => array(",
					"        'header'  => \"Content-type: application/x-www-form-urlencoded\r\n\"",
					"        'method'  => 'POST',",
					"        'content' => http_build_query($data)",
					"    )",
					");",
					"$context  = stream_context_create($options);",
					"$result = file_get_contents($url, false, $context);",
					"if ($result === FALSE) { /* Handle error */ }",
					"",
					"var_dump($result);",
					"?>"
				]
			}
		],
